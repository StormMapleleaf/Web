{
  "name": "mytheme",
  "version": "1.0.0",
  "description": "My WordPress Theme",
  "scripts": {
    "start": "chokidar \"less/**/*.less\" -c \"lessc less/style.less > style.css\""
  },
  "devDependencies": {
    "chokidar": "^3.5.2",
    "less": "^4.1.3"
  },
  "dependencies": {
    "autoprefixer": "^10.4.13",
    "chokidar-cli": "^3.0.0",
    "minimist": "^1.2.8",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-calc": "^8.2.4",
    "postcss-cli": "^10.1.0",
    "postcss-css-variables": "^0.18.0",
    "postcss-custom-media": "^9.1.2",
    "postcss-discard-duplicates": "^5.1.0",
    "postcss-focus-within": "^7.0.2",
    "postcss-merge-rules": "^5.1.4",
    "postcss-nested": "^6.0.1",
    "rtlcss": "^4.0.0"
  },
  "rtlcssConfig": {
    "options": {
      "autoRename": false,
      "autoRenameStrict": false,
      "blacklist": {},
      "clean": true,
      "greedy": false,
      "processUrls": false,
      "stringMap": []
    },
    "plugins": [],
    "map": false
  },
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ]
}
